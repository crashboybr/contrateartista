                		</div>
                		<div class="cat_row cat_row0">    
                			{% for cat in categories %}                  
                			<div class="cat_col">
                				<div class="cat_col_in">
                					<div class="title">
                						<a href="{{ path('contrate_frontend_search', {'category': cat.id }) }}"><img src="{{ asset(cat.pic) }}" alt="{{ cat.name }}"></a>
                						<div class="cat_title_desc">
                							<h2>
                								<a href="#">{{ cat.name }} <span>(3)</span></a>
                							</h2>
